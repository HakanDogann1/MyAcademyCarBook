@model MyAcademyCarBook.EntityLayer.Concrete.CarStatus
@{
    ViewData["Title"] = "CreateCarStatus";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>Yeni Araba Durumu Ekleme</h1>
<form method="post">
    <label>Araba Durum Başlığı</label>
    <input type="text" class="form-control" asp-for="CarStatusName" style="border:1px solid #222" />
    <br />
    <button class="btn btn-primary">Durum Ekle</button>
</form>
